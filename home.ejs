  border-radius: 5px;
}

/* Style the contact form */
input, button {
  display: block;
  margin: 10px 0;
  padding: 10px;
  width: 100%;
  border: 1px solid #ccc;
  border-radius: 5px;
}

/* Responsive design for smaller screens */
@media (max-width: 768px) {
  nav li {
    display: block;
    margin-bottom: 10px;
  }
}

</style>
